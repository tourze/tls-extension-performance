{
  "name": "tourze/tls-extension-performance",
  "description": "Performance-related TLS extensions for optimizing TLS handshake efficiency, reducing memory usage, and improving transmission performance",
  "type": "library",
  "license": "MIT",
  "require": {
    "tourze/enum-extra": "1.*",
    "tourze/tls-common": "0.0.*",
    "tourze/tls-extension-naming": "0.0.*",
    "tourze/tls-handshake-messages": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/framework-bundle": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-enum": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\TLSExtensionPerformance\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\TLSExtensionPerformance\\Tests\\": "tests/"
    }
  }
}